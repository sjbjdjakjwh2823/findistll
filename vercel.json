{
    "builds": [
        {
            "src": "api/handler.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.10"
            }
        },
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/handler.py"
        }
    ],
    "env": {
        "VERCEL_SB_URL": "@vercel_sb_url",
        "VERCEL_SB_KEY": "@vercel_sb_key",
        "VERCEL_SB_JWT_SECRET": "@vercel_sb_jwt_secret"
    }
}