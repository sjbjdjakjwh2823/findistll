{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index.py"
        }
    ],
    "functions": {
        "api/index.py": {
            "runtime": "python3.11",
            "maxDuration": 60
        }
    }
}